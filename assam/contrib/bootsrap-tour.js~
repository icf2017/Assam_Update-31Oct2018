var tour = new Tour({
  tour.addSteps([
  {
    element: ".menu_2",
    placement: "bottom",
    title: "Title of my step",
    content: "Content of my step"
  },
  {
    element: "#pathway_box",
    placement: "top",
    title: "Title of my step",
    content: "Content of my step"
  }
]) });	

// Initialize the tour
tour.init();

// Start the tour
tour.start();
